{
  "compilerOptions": {
    "target": "es6",
    "module": "system",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "removeComments": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "baseUrl": ".",
    "typeRoots" : ["./node_modules/@types"]
  },
  "exclude": [
    "node_modules"
  ]
}